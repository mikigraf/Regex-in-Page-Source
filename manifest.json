{
  "manifest_version" : 2,

  "name" : "HTML Regex Finder",
  "version" : "0.5.",
  "description" : "Find regular expressions in the HTML source code. ",

  "browser_action" : {
    "default_icon" : "icon.png",
    "default_popup" : "/html/popup.html"
  },

  "permissions" : [
    "tabs", "<all_urls>"
  ]

}
